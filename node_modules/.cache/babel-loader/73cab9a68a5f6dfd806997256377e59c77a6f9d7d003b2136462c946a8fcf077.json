":["<template>\n  <div>\n    <h1>Carrito de compra</h1>\n\n    <div class=\"container\">\n      <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12 d-flex\">\n        <table class=\"table\">\n          <thead>\n            <tr>\n              <th>Producto</th>\n              <th>Precio</th>\n              <th>Cantidad</th>\n              <th>Total</th>\n              <th></th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(item, index) in cartItems\" :key=\"item.name\">\n              <td>{{ item.name }}</td>\n              <td>{{ item.price }}</td>\n              <td>\n                <div class=\"row\">\n                  <div class=\"col-3\">\n                    <button\n                      @click=\"decrementQuantity(item)\"\n                      :disabled=\"item.quantity === 1\"\n                      class=\"btn btn-secondary mt-2 ms-2\"\n                      id=\"decrement-btn\"\n                    >\n                      -\n                    </button>\n                  </div>\n                  <div class=\"col-4\">\n                    <input\n                      v-model=\"item.quantity\"\n                      class=\"form-control\"\n                      :class=\"item.quantity > 0 ? 'positive' : 'negative'\"\n                      id=\"quantity-input\"\n                    />\n                  </div>\n                  <div class=\"col-3\">\n                    <button\n                      @click=\"incrementQuantity(item)\"\n                      class=\"btn btn-secondary mt-2\"\n                      id=\"increment-btn\"\n                    >\n                      +\n                    </button>\n                  </div>\n                </div>\n              </td>\n              <td>{{ getTotal(item) }}</td>\n              <td>\n                <button\n                  class=\"btn btn-danger\"\n                  @click=\"removeFromCart(item)\"\n                >\n                  Eliminar\n                </button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'CarritoCompra',\n  computed: {\n    cartItems() {\n      const cartItems = [];\n      const selectedItems = this.$store.getters.selectedItems;\n\n      // Agrupar los items por nombre y calcular la cantidad total\n      selectedItems.forEach(item => {\n        const existingItem = cartItems.find(i => i.name === item.name);\n        if (existingItem) {\n          existingItem.quantity += 1; // Incrementar la cantidad en 1\n        } else {\n          cartItems.push({...item, quantity: 1}); // Inicializar la cantidad en 1\n        }\n      });\n\n      return cartItems;\n    },\n  },\n  methods: {\n    decrementQuantity(item) {\n      if (item.quantity > 1) {\n        item.quantity--;\n      }\n    },\n    incrementQuantity(item) {\n      item.quantity++;\n    },\n    getTotal(item) {\n      return item.price * item.quantity;\n    },\n    removeFromCart(item) {\n      this.$store.dispatch('removeItem', item);\n    },\n  },\n};\n</script>\n\n<style scoped>\n#decrement-btn {\n  margin-left: 30px;\n}\n\n#quantity-input {\n  margin-left: 20px;\n  width: 50px;\n}\n</style>"],"mappings":";;;;;;;AAqEA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,QAAQ,EAAE;IACRC,SAAS,WAAAA,UAAA,EAAG;MACV,IAAMA,SAAQ,GAAI,EAAE;MACpB,IAAMC,aAAY,GAAI,IAAI,CAACC,MAAM,CAACC,OAAO,CAACF,aAAa;;MAEvD;MACAA,aAAa,CAACG,OAAO,CAAC,UAAAC,IAAG,EAAK;QAC5B,IAAMC,YAAW,GAAIN,SAAS,CAACO,IAAI,CAAC,UAAAC,CAAA;UAAA,OAAKA,CAAC,CAACV,IAAG,KAAMO,IAAI,CAACP,IAAI;QAAA,EAAC;QAC9D,IAAIQ,YAAY,EAAE;UAChBA,YAAY,CAACG,QAAO,IAAK,CAAC,EAAE;QAC9B,OAAO;UACLT,SAAS,CAACU,IAAI,CAAAC,aAAA,CAAAA,aAAA,KAAKN,IAAI;YAAEI,QAAQ,EAAE;UAAC,EAAC,CAAC,EAAE;QAC1C;MACF,CAAC,CAAC;;MAEF,OAAOT,SAAS;IAClB;EACF,CAAC;EACDY,OAAO,EAAE;IACPC,iBAAiB,WAAAA,kBAACR,IAAI,EAAE;MACtB,IAAIA,IAAI,CAACI,QAAO,GAAI,CAAC,EAAE;QACrBJ,IAAI,CAACI,QAAQ,EAAE;MACjB;IACF,CAAC;IACDK,iBAAiB,WAAA